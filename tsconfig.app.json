{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@GlobalModel/*": ["src/app/models/*"],
      "@GlobalInterface": ["src/app/interfaces"],
      "@GlobalUtilities": ["src/app/utilities"],
      "@GlobalService": ["src/app/service"],
      "@GlobalConst": ["src/app/const"]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
